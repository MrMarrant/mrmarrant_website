<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}MrMarrant{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {{ encore_entry_script_tags('portfolio') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <section>
                {% include '/portfolio/header.html.twig' %}
            </section>
            <hr>
            <section>
                {% include '/portfolio/aboutUs.html.twig' %}
            </section>
            <hr>
            <section>
                {% include '/portfolio/projects.html.twig' %}
            </section>
        {% endblock %}
    </body>
</html>
