<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}MrMarrant
			{% endblock %}
		</title>
		<link
		rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,400,0,0"/>
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			{{ encore_entry_link_tags('portfolio') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			{{ encore_entry_script_tags('portfolio') }}
		{% endblock %}
	</head>
	<body>
		{% block body %}
			<section>
				{% include '/portfolio/header.html.twig' %}
			</section>
			<section>
				{% include '/portfolio/aboutUs.html.twig' %}
			</section>
			<section>
				{% include '/portfolio/projects.html.twig' %}
			</section>
			<section>
				{% include '/portfolio/links.html.twig' %}
			</section>
			{% include '/portfolio/footer.html.twig' %}
		{% endblock %}
	</body>
</html>
