<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}MrMarrant{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            {{ encore_entry_script_tags('portfolio') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {{ encore_entry_script_tags('portfolio') }}
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div class="container-element">
                {% include '/portfolio/header.html.twig' %}
            </div>
            <hr>
            <div class="container-element">
                {% include '/portfolio/aboutUs.html.twig' %}
            </div>
        {% endblock %}
    </body>
</html>
